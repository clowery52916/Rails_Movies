<%= form_for @movies do |f| %>
  <%= f.label :title %>
  <%= f.text_field :name %>

  <%= f.label :genre %>
  <%= f.text_field :genre %>

  <%= f.label :photo %>
  <%= f.text_field :photo %>

  <%= f.label :year %>
  <%= f.text_field :year %>

  <%= f.label :synopis %>
  <%= f.text_field :synopis %>

  <%= f.label :add_to_favs %>
  <%= f.text_field :add_to_favs %>

  <%= f.submit%>
<% end %>
